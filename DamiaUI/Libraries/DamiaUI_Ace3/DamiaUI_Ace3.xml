<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/ ..\FrameXML\UI.xsd">
    <!--
        DamiaUI Embedded Ace3 Libraries Master Include
        
        Complete Ace3 library collection with DamiaUI namespace isolation.
        All libraries are compatible with WoW 11.2.
        
        Load order is important - dependencies must be loaded first.
    -->
    
    <!-- Core Libraries (Load First) -->
    <Include file="AceAddon-3.0\AceAddon-3.0.xml"/>
    <Include file="AceEvent-3.0\AceEvent-3.0.xml"/>
    
    <!-- Database and Console Libraries -->
    <Include file="AceDB-3.0\AceDB-3.0.xml"/>
    <Include file="AceConsole-3.0\AceConsole-3.0.xml"/>
    
    <!-- GUI Libraries (Load After Core) -->
    <Include file="AceGUI-3.0\AceGUI-3.0.xml"/>
    
    <!-- Configuration Libraries (Load After GUI) -->
    <Include file="AceConfig-3.0\AceConfig-3.0.xml"/>
    <Include file="AceDBOptions-3.0\AceDBOptions-3.0.xml"/>
    
</Ui>