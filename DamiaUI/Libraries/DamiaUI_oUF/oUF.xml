<Ui xmlns="http://www.blizzard.com/wow/ui/" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://www.blizzard.com/wow/ui/ ..\FrameXML\UI.xsd">
    <!--
        DamiaUI Embedded oUF Library
        
        A lightweight unit frame framework for World of Warcraft embedded within
        DamiaUI to avoid conflicts with standalone oUF installations.
        
        This is a complete, self-contained implementation compatible with WoW 11.2+
        and provides all the functionality needed for player, target, focus,
        party, and raid frames.
        
        Namespace: DamiaUI_oUF (LibStub registered as "oUF")
    -->
    
    <!-- Core oUF framework -->
    <Script file="oUF.lua"/>
    
    <!-- Tags system for flexible text formatting -->
    <Script file="tags.lua"/>
    
    <!-- Core elements -->
    <Script file="elements\health.lua"/>
    <Script file="elements\power.lua"/>
    <Script file="elements\castbar.lua"/>
    <Script file="elements\auras.lua"/>
    <Script file="elements\portrait.lua"/>
    <Script file="elements\name.lua"/>
    
</Ui>